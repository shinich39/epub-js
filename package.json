{
  "name": "epub-js",
  "version": "1.0.0",
  "description": "Build ePub document with javascript",
  "main": "./dist/epub.mjs",
  "exports": {
    ".": {
      "import": "./dist/epub.mjs",
      "require": "./dist/epub.cjs"
    }
  },
  "type": "module",
  "scripts": {
    "test": "npm run pretty && npm run build && node test/index.js",
    "build": "rollup --config rollup.config.js",
    "pretty": "prettier index.js src/index.js src/core --write --config prettier.config.js"
  },
  "keywords": [
    "javascript"
  ],
  "author": "shinich39 <shinich39@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "prettier": "^3.4.2",
    "rollup": "^4.29.1"
  },
  "dependencies": {
    "js-beautify": "^1.15.1",
    "mime": "^4.0.6"
  }
}